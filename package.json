{
  "name": "react-transcript-player",
  "version": "0.0.0",
  "description": "React component for playing audio with a synced transcript",
  "repository": {
    "type": "git",
    "url": "https://github.com/contours/react-transcript-player.git"
  },
  "main": "lib/player.js",
  "keywords": [
    "react-component",
    "audio",
    "transcript"
  ],
  "author": "Ryan Shaw <ryanshaw@unc.edu>",
  "license": "ISC",
  "peerDependencies": {
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel": "^5.5.6",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "tape": "^4.0.0",
    "watchy": "^0.6.2"
  },
  "scripts": {
    "lint": "eslint --ext .jsx src",
    "transpile": "babel src -d lib --optional runtime --source-maps inline",
    "transpile-tests": "npm -s run transpile && babel test -d lib/test",
    "test": "npm -s run transpile-tests && tape lib/test/*.js",
    "build": "npm -s run lint && npm -s run transpile",
    "build-watch": "watchy -s -w src -i '/#|/flycheck_' -- npm -s run build"
  },
  "dependencies": {
    "babel-runtime": "^5.6.3",
    "functify": "file:../functify",
    "immutable": "^3.7.4",
    "interval-tree-1d": "^1.0.3"
  }
}
