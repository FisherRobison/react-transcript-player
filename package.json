{
  "name": "react-transcript-player",
  "version": "0.0.0",
  "description": "React component for playing audio with a synced transcript",
  "repository": {
    "type": "git",
    "url": "https://github.com/contours/react-transcript-player.git"
  },
  "main": "lib/player.js",
  "keywords": [
    "react-component",
    "audio",
    "transcript"
  ],
  "author": "Ryan Shaw <ryanshaw@unc.edu>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-1": "^6.1.2",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^3.8.0",
    "tape": "^4.2.2",
    "watchy": "^0.6.5"
  },
  "browserify": {
    "transform": [["babelify"]]
  },
  "scripts": {
    "lint": "eslint --ext .jsx src",
    "transpile": "babel src -d lib --source-maps inline",
    "transpile-tests": "npm -s run transpile && babel test -d lib/test",
    "test": "npm -s run transpile-tests && tape lib/test/*.js",
    "build": "npm -s run lint && npm -s run transpile",
    "build-watch": "watchy -s -w src -i '/#|/flycheck_' -- npm -s run build"
  },
  "dependencies": {
    "babel-runtime": "^6.0.14",
    "functify": "^0.3.0",
    "immutable": "^3.7.5",
    "interval-tree-1d": "^1.0.3"
  },
  "peerDependencies": {
    "react": "^0.14.5",
    "react-dom": "^0.14.5"
  }
}
