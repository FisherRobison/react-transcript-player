    "react-component",
    "audio",
    "transcript"
  ],
  "author": "Ryan Shaw <ryanshaw@unc.edu>",
  "license": "ISC",
  "peerDependencies": {
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel": "^5.5.6",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "watch": "^0.16.0"
  },
  "scripts": {
    "lint": "eslint --ext .jsx src",
