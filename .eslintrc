{
  "plugins": [ "react" ],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "comma-style": [2, "first"],
    "curly": [2, "multi-line"],
    "eol-last": 2,
    "new-cap": 0,
    "no-mixed-requires": 0,
    "no-multiple-empty-lines": [2, {"max": 1}],
    "no-process-exit": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": [2, "nofunc"],
    "quotes": 0,
    "semi": 0
  }
}
